<div class="table-container">
    <div class="custom-table-search">
        <input type="text" id="search-input" placeholder="Search..." />
        <button id="search-button">Search</button>
        <button id="clear-search-button">Clear</button>
    </div>
    <div class="custom-table">
        <div class="custom-table-header">
            {{#columns}}
            <div class="custom-table-header-cell">{{.}}</div>
            {{/columns}}
        </div>
        <div class="custom-table-body" id="table-body">
            {{#rows}}
            <div class="custom-table-row" id="{{id}}">
                {{#cells}}
                <div class="custom-table-cell">{{{.}}}</div>
                {{/cells}}
            </div>
            {{/rows}}
        </div>
    </div>
    <div class="pagination" id="{{paginationid}}">
        <button class="pagination-button" id="prev-page" disabled>Previous</button>
        <span id="current-page">Page {{currentPage}} of {{totalPages}}</span>
        <button class="pagination-button" id="next-page">Next</button>
    </div>
</div>